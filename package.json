{
  "name": "lingvokot-github-io",
  "version": "0.6.0",
  "private": true,
  "description": "Static website hosten on GH Pages build with modern FrontEnd stack",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:karma:sauce && npm run lint",
    "deploy": "npm run build && git add dist index.html && git commit -m 'Update build' && npm version minor && git push && git push --tags",
    "test:karma:sauce": "karma start karma.conf-sauce.js",
    "test:karma": "karma start --single-run",
    "watch": "webpack-dev-server --watch --color --hot --inline --host 0.0.0.0",
    "build": "node scripts/build.js && cp assets/privacy_policy.html ./policy.html",
    "update-apps-info": "node --harmony ./scripts/update-apps-info.js",
    "static": "static",
    "lint": "eslint --cache --color --config .eslintrc 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lingvokot/lingvokot.github.io.git"
  },
  "keywords": [
    "mocha",
    "chai",
    "quixote",
    "webpack",
    "react",
    "static website"
  ],
  "author": "Lingvokot <https://github.com/Lingvokot/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lingvokot/lingvokot.github.io/issues"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "homepage": "https://github.com/Lingvokot/lingvokot.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "img-loader": "^2.0.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "jsdom": "^11.6.1",
    "json-format": "^1.0.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-quixote": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "minimize": "^2.2.0",
    "mocha": "^5.0.0",
    "node-static": "^0.7.10",
    "prop-types": "^15.6.0",
    "quixote": "^0.14.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "reset.css": "^2.0.2",
    "rimraf": "^2.6.2",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "static-site-generator-webpack-plugin": "^3.4.1",

    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "form-serialize": "^0.7.2",
    "jsonp": "^0.2.1",
    "nuka-carousel": "^3.0.1",
    "react-scroll": "^1.7.6",
    "reqwest": "^2.0.5"
  }
}

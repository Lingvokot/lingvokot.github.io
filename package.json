{
  "name": "lingvokot-github-io",
  "version": "0.3.0",
  "private": true,
  "description": "Static website hosten on GH Pages build with modern FrontEnd stack",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:karma:sauce && npm run lint",
    "deploy": "npm run build && git add dist index.html && git commit -m 'Update build' && npm version minor && git push && git push --tags",
    "test:karma:sauce": "karma start karma.conf-sauce.js",
    "test:karma": "karma start --single-run",
    "watch": "webpack-dev-server --watch --color --hot --inline --host 0.0.0.0",
    "build": "node scripts/build.js && cp assets/privacy_policy.html ./policy.html",
    "static": "static",
    "lint": "eslint --cache --color --config .eslintrc 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lingvokot/lingvokot.github.io.git"
  },
  "keywords": [
    "mocha",
    "chai",
    "quixote",
    "webpack",
    "react",
    "static website"
  ],
  "author": "Lingvokot <https://github.com/Lingvokot/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lingvokot/lingvokot.github.io/issues"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "homepage": "https://github.com/Lingvokot/lingvokot.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "img-loader": "^2.0.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-quixote": "^0.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "minimize": "^2.1.0",
    "mocha": "^3.2.0",
    "node-static": "^0.7.9",
    "prop-types": "^15.5.8",
    "quixote": "^0.12.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-scroll": "^1.5.2",
    "reset.css": "^2.0.2",
    "rimraf": "^2.6.1",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.1",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.16.1",
